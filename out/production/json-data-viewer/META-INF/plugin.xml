<idea-plugin version="2">
  <id>com.ssanjun.intellij.datagrip.jsonDataViewer</id>
  <name>JSON Data Viewer for DataGrip</name>
  <version>1.0</version>
  <vendor email="ssanjun.dev@gmail.com" url="https://www.ssanjun.com">ssanjun</vendor>

  <description><![CDATA[
      JSON Data Viewer for DataGrip<br>
      <em>Supported Postgresql</em>
    ]]></description>

  <change-notes><![CDATA[
      v0.0.1 init commit
    ]]>
  </change-notes>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="145" until-build="145.*"/>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <!-- uncomment to enable plugin in all products
  <depends>com.intellij.modules.lang</depends>
  -->

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
    <toolWindow id="JSON Data Viewer" secondary="true" icon="/jsonDataViewer/plus.png" anchor="right"
                  factoryClass="com.ssanjun.intellij.datagrip.jsonDataViewer.jsonDataViewerFactory"/>

  </extensions>

  <actions>
    <!-- Add your actions here -->
    <!--action id="jsonDataViewer.MyAction" class="com.ssanjun.intellij.datagrip.jsonDataViewer.jsonDataViewer"
            text="JSON Viewer" description="The action Implemented by jsonDataViewer class">
      <add-to-group group-id="ViewMenu" anchor="first"/>
    </action-->
  </actions>

</idea-plugin>